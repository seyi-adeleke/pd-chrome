{
    "name": "Please I want to be productive",
    "version": "1.0.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "storage"
    ],
    "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+F"
          },
          "description": "Opens index.html"
        }
      }
}